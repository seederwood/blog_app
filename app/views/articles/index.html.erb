<h1 id="page_title">Stuff I've Written</h1>

<div id="posts_wrapper" class="skinny_wrapper">
  <% @articles.each do |article| %>
    <div class="post">
      <%= article.created_at.strftime("%^a, %d %B | %H:%M") %>
      <h2><%= link_to article.title, article %></h2>
      <hr />
    </div>
  <% end %>
</div>

<%= will_paginate @articles %>
